                <td>Эквивалентный диаметр воздушных каналов </td>
                <td><input type="number" step="any" name="data.DDVK" value="0.0565" /></td>
                <td>м</td>
            </tr>
            <tr>
                <td>Толщина стенки выполненного из шмота </td>
                <td><input type="number" step="any" name="data.S" value="0.02" /></td>
                <td>м</td>
            </tr>
            <tr>
                <td>Размер блока </td>
                <td><input type="number" step="any" name="data.FIBL" value="0.161" /></td>
                <td>м</td>
            </tr>

        </table>

        <div style="text-align: right;
                margin-left: 30%;
                width: 40%;
                padding:1%">
            <button class="btn btn-primary" type="submit" data-toggle="modal" data-target="#exampleModal">Вычислить результа</button>
            <button class="btn btn-warning" type="reset"> Очистить таблицу</button>
        </div>

       
     </form>
</div>
 


